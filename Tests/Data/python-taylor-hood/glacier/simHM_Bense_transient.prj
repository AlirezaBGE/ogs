    <parameters> <!--to do: values-->
        <!-- Material parameters -->
        <parameter>
            <name>YoungModulus</name>
            <type>Group</type>
            <group_id_property>MaterialIDs</group_id_property>
            <index_values>
                <index>0</index>
                <value>60e6</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>1</index>
                <value>60e7</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>2</index>
                <value>60e6</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>3</index>
                <value>60e7</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>4</index>
                <value>60e6</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>5</index>
                <value>60e7</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>6</index>
                <value>60e8</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>7</index>
                <value>60e9</value> <!--Pa-->
            </index_values>
        </parameter>
        <parameter>
            <name>PoissonRatio</name>
            <type>Group</type>
            <group_id_property>MaterialIDs</group_id_property>
            <index_values>
                <index>0</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>1</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>2</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>3</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>4</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>5</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>6</index>
                <value>0.35</value>
            </index_values>
            <index_values>
                <index>7</index>
                <value>0.45</value>
            </index_values>
        </parameter>
        <parameter>
            <name>ViscosityWater</name>
            <type>Constant</type>
            <value> 1.e-3 </value> <!--Pa*s-->
        </parameter>
        <parameter>
            <name>DensityWater</name>
            <type>Constant</type>
            <value> 1.e3 </value> <!--kg/m³-->
        </parameter>
        <parameter>
            <name>DensityWaterFunction</name>
            <type>CurveScaled</type>
            <curve>DensityWaterCurve</curve>
            <parameter>DensityWater</parameter>
        </parameter>
        <parameter>
            <name>ReferenceTemperature</name>
            <type>Constant</type>
            <value>293.15</value> <!--K--> 
        </parameter>
        <!-- Initial and boundary values -->
        <parameter>
            <name>InitialPressure</name>
            <type>Constant</type>
            <values>0</values>
        </parameter>
        <parameter>
            <name>AmbientPressure</name>
            <type>Constant</type>
            <values>0</values>
        </parameter>
        <parameter>
            <name>LinearPressureProfile</name>
            <type>Function</type>
            <expression>-1000*9.81*y</expression>
        </parameter>
        <parameter>
            <name>InitialDisplacement</name>
            <type>Constant</type>
            <values>0 0</values>
        </parameter>
        <parameter>
            <name>ZeroDisplacement</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
    </parameters>
    <curves>
        <curve>
            <name>DensityWaterCurve</name>
            <coords>0.0 0.5 1.0</coords>
            <values>0.0 0.99 0.99</values>
        </curve>
    </curves>
    <process_variables>
        <process_variable>
            <name>pressure</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>InitialPressure</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>basinmesh_boundary_top</mesh>
                    <type>Dirichlet</type>
                    <parameter>AmbientPressure</parameter>
                </boundary_condition>
                <!-- linear (hydrostatic) pressure profile left and right -->
                <boundary_condition>
                    <mesh>basinmesh_boundary_left</mesh>
                    <type>Dirichlet</type>
                    <parameter>AmbientPressure</parameter> <!--LinearPressureProfile-->
                </boundary_condition>
                <boundary_condition>
                    <mesh>basinmesh_boundary_right</mesh>
                    <type>Dirichlet</type>
                    <parameter>AmbientPressure</parameter> <!--LinearPressureProfile-->
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
        <process_variable>
            <name>displacement</name>
            <components>2</components>
            <!--for  displacement, the element order must be 2, because the mesh consists of the 6 node triangles-->
            <order>2</order>
            <initial_condition>InitialDisplacement</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>basinmesh_boundary_left</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>ZeroDisplacement</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>basinmesh_boundary_right</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>ZeroDisplacement</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>basinmesh_boundary_bottom</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>ZeroDisplacement</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>basinmesh_boundary_top</mesh>
                    <type>Python</type>
                    <component>1</component>
                    <bc_object>bc_y</bc_object> <!--the python object is created using timeBCs_glacier.py (see L. 10)-->
                    <flush_stdout>true</flush_stdout> <!-- for debugging: false -->
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
    </process_variables>
    <time_loop>
        <processes>
            <process ref="HM">
                <nonlinear_solver>basic_newton</nonlinear_solver>
                <convergence_criterion>
                    <type>PerComponentDeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <reltols>1e-10 1e-10 1e-10</reltols>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>0</t_initial>
                    <t_end>10</t_end>
                    <!-- purely pro-elastic problem: time scale irrelevant -->
                    <timesteps>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>100</delta_t>
                            <!-- i.e.: repeat one time a time step with size of delta_t -->
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>OGSoutput_sim_HM</prefix>
            <timesteps>
                <pair>
                    <repeat>100</repeat>
                    <each_steps>1</each_steps>
                    <!-- i.e.: do a hundred times the output at each step -->
                </pair>
            </timesteps>
            <variables>
                <variable>displacement</variable>
                <variable>sigma</variable>
            </variables>
            <suffix>_ts_{:timestep}_t_{:time}</suffix>
        </output>
    </time_loop>    
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_newton</name>
            <type>Newton</type>
            <max_iter>20</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <eigen>
                <!--                <solver_type>PardisoLU</solver_type>-->
                <solver_type>SparseLU</solver_type>
                <scaling>true</scaling>
            </eigen>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>
